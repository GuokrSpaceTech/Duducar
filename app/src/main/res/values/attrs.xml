<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- shswitchview -->
    <declare-styleable name="ShSwitchView">
        <attr name="tintColor" format="reference|color" />
        <attr name="outerStrokeWidth" format="reference|dimension" />
        <attr name="shadowSpace" format="reference|dimension" />
    </declare-styleable>

    <!-- flowlayout -->
    <declare-styleable name="TagFlowLayout">
        <attr name="auto_select_effect" format="boolean"></attr>
        <attr name="max_select" format="integer"></attr>
        <attr name="gravity" format="string"></attr>
    </declare-styleable>
</resources>